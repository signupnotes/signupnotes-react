{
  "name": "@signupnotes/signupnotes-react",
  "private": false,
  "description": "A signupnotes form wrapper for React",
  "version": "0.1.2",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm tsc -p tsconfig.app.json & vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "publish": "pnpm build & npm publish --access public"
  },
  "dependencies": {
    "@types/node": "^20.14.12",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite-plugin-dts": "4.0.0-beta.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signupnotes/signupnotes-react.git"
  },
  "keywords": [
    "signupnotes"
  ],
  "author": "kovan studio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signupnotes/signupnotes-react/issues"
  },
  "homepage": "https://github.com/signupnotes/signupnotes-react#readme"
}
